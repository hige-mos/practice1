   "source": [
    "def getpos(ss,control,drop):\n",
    "    for line in open('./pos/' + str(ss) + '_sample_Drop'+ str(drop) +'.pos', 'r',encoding=\"utf-8_sig\").readlines():\n",
    "        data = line[:-1].split()\n",
    "        node = int(data[0])\n",
    "        if node == 1: continue\n",
    "        dframe.loc[ss,control,node]['x'] = float(data[2])\n",
    "        dframe.loc[ss,control,node]['x'] = float(data[3])\n",
    "        dframe.loc[ss,control,node]['x'] = float(data[4])\n",
    "        if (node < ssinfo[ss]['apnum'] + 2):\n",
    "            dframe.loc[ss,control,node]['Uact'] = dframe.loc[ss,control,node]['Dact'] = 'AP'\n",
    "        else:\n",
    "            dframe.loc[ss,control,node]['Uact'] = dframe.loc[ss,control,node]['Dact'] = 'STA'"
   ]
  },
